<% for(var i=0; i<users.length; i++) { %>
  <% if (!users[i].admin) { %>
    <div class=”col">
      <h3><span style="float:left;"><%= users[i].name %></span><span style="float:right;">Round 1 <%= users[i].round1 %> (Total Balance <%= users[i].balance %>)</span></h3>
      <br/>
      <br/>
      <ol>
      <% users[i].team.forEach(function (playerId) { %>
        <% players.forEach(function (player) { %>
          <% if (player.id == playerId) { %>
            <li><span style="float:left;"><%= player.name %></span><span style="float:right;">$<%= player.price %></span></li>
          <% } %>
        <% }) %>
      <% }) %>
      </ol>
    </div>
  <% } %>
<% } %>




